{
  "compilerOptions": {
    "module": "es2022",
    "target": "es2024",
    "moduleResolution": "nodenext",

    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "jsx": "preserve",

    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },

  "exclude": []
}
